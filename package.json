{
  "name": "hello-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.34",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "graphql": "^15.5.2",
    "kidsloop-audio-player": "bitbucket:calmisland/kidsloop-audio-player",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "HOST=local.alpha.kidsloop.net HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format:check": "prettier --check \"{src,tests}/**/*.{ts,tsx}\" \"*.json\"",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx}\" \"*.json\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "proxy": "http://localhost:8080/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^11.2.4",
    "prettier": "^2.4.1"
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "{src,tests}/**/*.{ts,tsx}": "prettier --write"
  }
}
